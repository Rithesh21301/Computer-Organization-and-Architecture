.section .text
.global main

main:
    la t0 ,val1
    ld t0 ,0(t0)

    la t1 ,val2
    ld t1 ,0(t1)

    mul t3 ,t0 ,t1
    la t4 ,r1
    sd t3, 0(t4)

    mulhu t5 ,t0 ,t1
    la t6 ,r2
    sd t5 ,0(t6)






.section .data
val1:  .dword 0x1234567887654321
val2:  .dword 0x8765432112345678
result: .dword 0
r1: .dword 0
r2: .dword 0
